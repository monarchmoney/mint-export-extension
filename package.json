{
  "name": "mint-export-extension",
  "version": "1.0.6",
  "description": "Browser extension to export your data from Mint. Built by the team at Monarch Money.",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:beta": "tsc --noEmit && cross-env IS_BETA=1 vite build",
    "pack": "pnpm build && zip -r chrome-extension.zip dist",
    "pack:beta": "pnpm build:beta && zip -r chrome-extension.zip dist",
    "build:firefox": "tsc --noEmit && cross-env __FIREFOX__=true vite build",
    "build:watch": "cross-env __DEV__=true vite build -w --mode development",
    "build:firefox:watch": "cross-env __DEV__=true __FIREFOX__=true vite build -w --mode development",
    "build:hmr": "rollup --config utils/reload/rollup.config.ts",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "pnpm build:hmr && (run-p wss build:watch)",
    "dev:firefox": "pnpm build:hmr && (run-p wss build:firefox:watch)",
    "test": "jest",
    "commitlint": "commitlint --edit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier . --write",
    "prepare": "husky install",
    "release": "release-it",
    "release:beta": "pnpm release --preRelease=beta --'hooks.after:bump=\"pnpm run pack:beta\"'"
  },
  "type": "module",
  "dependencies": {
    "@sentry/browser": "^7.80.0",
    "@sentry/react": "^7.80.0",
    "@sentry/vite-plugin": "^2.10.0",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.0.0",
    "construct-style-sheets-polyfill": "^3.1.0",
    "jszip": "3.10.1",
    "luxon": "3.4.3",
    "pluralize": "^8.0.0",
    "postcss": "^8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.5",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@testing-library/react": "13.4.0",
    "@types/chrome": "0.0.251",
    "@types/jest": "29.5.8",
    "@types/luxon": "3.3.4",
    "@types/node": "18.15.11",
    "@types/pluralize": "^0.0.33",
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.15",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitejs/plugin-react": "2.2.0",
    "chokidar": "^3.5.3",
    "chrome-webstore-upload": "^1.0.0",
    "chrome-webstore-upload-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "11.1.0",
    "husky": "^8.0.0",
    "isomorphic-fetch": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.5.0",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "release-it": "^17.0.0",
    "rollup": "2.79.1",
    "sass": "1.62.1",
    "ts-jest": "29.0.2",
    "ts-loader": "9.4.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "4.9.5",
    "vite": "3.2.7",
    "ws": "8.13.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "packageManager": "pnpm@8.9.2"
}
